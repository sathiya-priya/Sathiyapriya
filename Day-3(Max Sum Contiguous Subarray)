public class Solution {
    public int maxSubArray(final List<Integer> A) {
       int sum=0;
    int mainsum=0;
    int count=0;
    for(int i=0;i<A.size();i++)
    {
        sum=sum+A.get(i);
        if(sum<0)
        {
        count++;
        sum=0;
        }
        if(mainsum<sum)
        mainsum=sum;
    }
    if(count==A.size())
    {
         Collections.sort(A);
         mainsum=A.get(A.size()-1);
    }
    return mainsum;
    
}
}
